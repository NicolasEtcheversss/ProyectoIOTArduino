void setup()
{
  pinMode(3, OUTPUT);
  pinMode(4, OUTPUT);
  pinMode(5, OUTPUT);
  pinMode(6, OUTPUT);
  pinMode(7, OUTPUT);
  pinMode(8, OUTPUT);
  pinMode(9, OUTPUT);
  pinMode(10, OUTPUT);
}

void loop()
{
  for (int i = 9; i >= 1; i--) {
    // Enciende el LED correspondiente al número actual de la cuenta regresiva
    digitalWrite(i, HIGH);
    
    // Espera un segundo
    delay(1000);
    
    // Apaga el LED
    digitalWrite(i, LOW);
    
    // Espera un breve período antes de pasar al siguiente número
    delay(200);
  }
}


Y este otro: 

// C++ code
